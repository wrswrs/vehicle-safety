    "test": "vue-cli-service test:unit",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|ts|vue)' --locales './src/locales/**/*.json'",
    "tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx,.vue}'"
  },
  "dependencies": {
